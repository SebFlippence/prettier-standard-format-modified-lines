{
  "name": "prettier-standard-format-modified-lines",
  "displayName": "Prettier Standard - format modified lines",
  "description": "Formats modified lines only via the experimental prettier-standard --lines flag or formats the selected text via prettier-standard",
  "version": "0.0.1",
  "publisher": "sebflipper",
  "homepage": "https://marketplace.visualstudio.com/items?itemName=sebflipper.prettier-standard-format-modified-lines",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebflipper/prettier-standard-format-modified-lines.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebflipper/prettier-standard-format-modified-lines/issues"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:json5",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less",
    "onLanguage:graphql",
    "onLanguage:markdown",
    "onLanguage:mdx",
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:yaml",
    "onLanguage:mdx",
    "onLanguage:flow",
    "onLanguage:angular"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "prettier-standard": "^16.2.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vsce": "^1.75.0",
    "vscode-test": "^1.3.0"
  }
}
