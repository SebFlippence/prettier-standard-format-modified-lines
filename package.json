{
  "name": "prettier-format-modified-lines",
  "displayName": "prettier-format-modified-lines",
  "description": "Only run Prettier over modified lines",
  "version": "0.0.1",
	"publisher": "sebflipper",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:json5",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less",
    "onLanguage:graphql",
    "onLanguage:markdown",
    "onLanguage:mdx",
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:yaml",
    "onLanguage:mdx",
    "onLanguage:flow",
    "onLanguage:angular"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "micromatch": "^4.0.2",
    "parse-diff": "^0.7.0",
    "prettier": "^2.0.4",
    "prettier-standard": "^16.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/prettier": "^2.0.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
